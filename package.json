{
  "name": "@ogcio/building-blocks-sdk",
  "version": "0.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "rm -rf dist && tsc -p tsconfig.prod.json",
    "clients:update": "node --import=tsx src/cli/index.ts clients:update -c src/clients-configurations/clients-configuration.json",
    "clients:outdated": "node --import=tsx src/cli/index.ts clients:outdated -c src/clients-configurations/clients-configuration.json",
    "check:formatting": "biome format",
    "fix:formatting": "biome format --write",
    "check:linting": "biome lint",
    "fix:linting": "biome lint --write",
    "prepare": "husky || true",
    "services:feature-flags:start:logto": "cd ./services/feature-flags && docker-compose up -d && npm run start:auth:logto",
    "services:feature-flags:start:local": "cd ./services/feature-flags && docker-compose up -d && npm run start:auth:local"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.33.15",
    "commander": "^12.1.0",
    "http-errors": "^2.0.0",
    "openapi-fetch": "^0.12.2",
    "openapi-typescript": "^7.4.1",
    "openapi-typescript-helpers": "^0.0.13",
    "safe-stable-stringify": "^2.5.0",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/http-errors": "^2.0.4",
    "commitlint": "^19.5.0",
    "husky": "^9.1.6",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "peerDependencies": {
    "@logto/node": "2.5.5",
    "unleash-client": "^6.1.1"
  },
  "peerDependenciesMeta": {
    "@logto/node": {
      "optional": true
    },
    "unleash-client": {
      "optional": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
